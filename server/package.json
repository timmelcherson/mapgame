{
  "name": "mapgame",
  "version": "1.0.0",
  "description": "MERN stack geography game",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd ../client && npm start",
    "dev": "concurrently \" npm run server\" \"npm run client\""
  },
  "author": "Tim Melcherson",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^4.1.1",
    "express": "^4.17.1",
    "mongoose": "^5.7.5"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  },
  "proxy": "https://localhost:5000"
}
